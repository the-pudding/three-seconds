#average
	.g-axis
		line
			stroke-opacity 0.33
			stroke $bg-invert
		path
			display none
		text
			opacity 0
	.foul
		circle
			stroke $fg
			fill $bg
			stroke-opacity 0.5
	
		text
			fill $fg
			fill-opacity 0.5
	
		&.is-highlight
			circle
				fill $primary
				stroke-opacity 1
			text
				fill $fg
				fill-opacity 1
			
	.quarter
		fill $bg-invert